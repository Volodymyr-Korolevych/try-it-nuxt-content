{
  "name": "nuxt-content",
  "private": true,
  "description": "Write your content inside your Nuxt app",
  "repository": "nuxt/content",
  "license": "MIT",
  "contributors": [
    {
      "name": "NuxtJS Team"
    }
  ],
  "scripts": {
    "dev": "nuxt --port 4040",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "docs": "nuxt docs",
    "lint": "eslint --ext .js,.vue .",
    "release": "yarn test && lerna publish",
    "test": "yarn lint && yarn test:packages",
    "test:watch": "jest packages --watch",
    "test:packages": "jest packages -i",
    "test:packages:content": "jest packages/content -i",
    "test:packages:create-nuxt-content-docs": "jest packages/create-nuxt-content-docs"
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "core-js": "^3.15.1",
    "nuxt": "^2.15.7",
    "vue-codemirror": "^4.0.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^4.2.0",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "eslint": "^7.29.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "jest": "^27.0.5",
    "postcss": "^8.3.5",
    "vue-jest": "^3.0.4"
  }
}
